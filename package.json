    "glob": "^10.3.10",
    "terser": "^5.26.0",
    "clean-css": "^5.3.3",
    "sharp": "^0.33.2",
    "html-minifier-terser": "^7.2.0"
  },
  "engines": {
    "node": ">=16"
  }
}
