{
  "name": "maxnate-website",
  "version": "1.0.0",
  "description": "Maxnate Africa multi-tenant website with Decap CMS",
  "scripts": {
    "build": "node scripts/build-dist.js",
    "build:content": "node scripts/build-content.js",
    "build:admin": "node scripts/build-admin.js",
